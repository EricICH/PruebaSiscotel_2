<p id="notice"><%= notice %></p>

<h1>Clientes</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Tipo</th>
      <th colspan="2">Doc</th>
      <th>Emision</th>
      <th>Vencimiento</th>
      <th>Telefono</th>
      <th>Tlf Secundario</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clients.each do |client| %>
      <tr>
        <td><%= client.first_name %></td>
        <td><%= client.last_name %></td>
        <td><%= client.type %></td>
        <td><%= client.ddoc_type %></td>
        <td><%= client.doc %></td>
        <td><%= client.docEm %></td>
        <td><%= client.docExp %></td>
        <td><%= client.phone %></td>
        <td><%= client.sec_phone %></td>
        <td><%= link_to 'Ver', client %></td>
        <td><%= link_to 'Editar', edit_client_path(client) %></td>
        <td><%= link_to 'Borrar', client, method: :delete, data: { confirm: 'seguro desea Eliminar Este registro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Cliente', new_client_path %>
